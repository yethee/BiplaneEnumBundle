{
    "name": "yethee/enum-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides a typed enumeration for Symfony2 applications",
    "keywords": ["typed enumeration"],
    "license": "MIT",
    "authors": [
        { "name": "Denis Vasilev", "email": "liquid.yethee@hotmail.com" }
    ],

    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.1,<2.3-dev",
        "symfony/form": ">=2.1,<2.3-dev"
    },

    "require-dev": {
        "jms/serializer-bundle": "1.0.*",
        "symfony/serializer": ">=2.1,<2.3-dev"
    },

    "suggest": {
        "jms/serializer-bundle": "1.0.*",
        "symfony/serializer": ">=2.1,<2.3-dev"
    },

    "conflict": {
        "jms/serializer-bundle": "<1.0-dev"
    },

    "minimum-stability": "dev",

    "autoload": {
        "psr-0": { "Biplane\\EnumBundle": "" }
    },

    "target-dir": "Biplane/EnumBundle",

    "extra": {
        "branch-alias": {
          "dev-master": "1.2-dev"
        }
    }
}
